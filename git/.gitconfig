[user]
	email = andrejlevkovitch@gmail.com
	name = andrejlevkovitch
[push]
	default = simple
[init]
	templatedir = 
[core]
	editor = vim
[diff]
  external = git_diff_wrapper
[difftool]
  trustExitCode = true
[pager]
  diff = 
[merge]
  tool = vimdiff
[mergetool]
  trustExitCode = true
[alias]
  tree = log --graph --abbrev-commit --decorate --format=format:'%C(bold blue)%h%C(reset) - %C(bold cyan)%aD%C(reset) %C(bold green)(%ar)%C(reset)%C(bold yellow)%d%C(reset)%n''          %C(white)%s%C(reset) %C(dim white)- %an%C(reset)' --all
